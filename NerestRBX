local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "NerestRBX", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = true, IntroText = "Loading..."})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Fly (не воркает)",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/qw1dixx/NerestRBXF/main/NerestRBXFly"))()
  	end    
})



























































































OrionLib:MakeNotification({
	Name = "NerestRBX",
	Content = "Loaded!",
	Image = "",
	Time = 5
})

OrionLib:Init()
