local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "NerestRBX", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = true, IntroText = "Loading..."})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Fly (не воркает)",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/qw1dixx/NerestRBXF/main/NerestRBXFly"))()
  	end    
})

local Section = Tab:AddSection({
	Name = "Aimbot"
})

Tab:AddButton({
	Name = "Aimbot",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/qw1dixx/NerestRBX_Functions/main/NerestRBX_Aimbot"))()
  	end    
})

Tab:AddSlider({
	Name = "GravityHack (normal is 196)",
	Min = 0,
	Max = 500,
	Default = 196,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "",
	Callback = function(Value)
		workspace.Gravity = Value
	end    
})





























































































OrionLib:MakeNotification({
	Name = "NerestRBX",
	Content = "Loaded!",
	Image = "",
	Time = 5
})

OrionLib:Init()
